<style>
    .music-track{
        padding: 10px;
        background: #f7f8f9;
        border: 1px solid #dce1e6;
        border-radius: 3px;
        min-height: 100px;
        overflow: hidden;
        padding: 18px;
        position: relative;
        z-index: 2;
        margin-bottom: 2rem;

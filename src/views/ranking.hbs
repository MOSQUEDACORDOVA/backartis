    }
    .share-music i{
        color: white;
        background-color: #00000082;
        padding: 7px;
        border-radius: 20%;
        font-size: 14px;
    }
    .play--titulo i{
        background: #D9AD26;
        color: white;
        padding:1rem;
        clip-path: circle(50% at 50% 50%);
